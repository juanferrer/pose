<UserControl x:Class="Pose.Panels.Properties.SubPanels.TranslateSubPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:Pose.Controls"
             xmlns:properties="clr-namespace:Pose.Panels.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="0,0,32,0">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <properties:PropertyFieldView Grid.Row="0" DataContext="{Binding X}" />
        <properties:PropertyFieldView Grid.Row="1" DataContext="{Binding Y}" />
    </Grid>
</UserControl>
